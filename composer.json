    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "joseph-lenton/php-error": "dev-master",
        "phpunit/phpunit": "^8",
        "miaoxing/dev": "^6.3.2"
    },
    "authors": [
        {
            "name": "Twin Huang",
            "email": "twinhuang@qq.com",
            "homepage": "https://github.com/twinh/wei"
        }
    ],
    "autoload": {
        "files": [
            "lib/Wei.php"
        ]
    },
    "autoload-dev": {
